{
  "name": "playsoccer",
  "private": true,
  "version": "1.0.0",
  "description": "PlaySoccer",
  "repository": "",
  "license": "UNLICENSED",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config ./build/webpack.dev.js",
    "build-host": "cross-env NODE_ENV=development webpack-dev-server --host 0.0.0.0 --config ./build/webpack.dev_host.js",
    "build-dev-prod": "cross-env NODE_ENV=development webpack-dev-server --config ./build/webpack.dev_prod.js",
    "build-dev": "cross-env NODE_ENV=development node ./build/build.js",
    "build-prod": "cross-env NODE_ENV=production node ./build/build.js",
    "upload-prod": "npm run build-prod && scp -r www ari@161.35.23.238:/home/ari/PlaySoccer/app/",
    "ios": "npm run build-dev-cordova-ios && framework7 cordova run ios",
    "cap-ios-build": "npm run build-dev && npx cap copy ios && npx cap open ios",
    "cap-ios-prod": "npm run build-prod && npx cap copy ios && npx cap open ios",
    "build-dev-cordova": "cross-env TARGET=cordova cross-env NODE_ENV=development node ./build/build.js && framework7 cordova build",
    "build-prod-cordova": "cross-env TARGET=cordova cross-env NODE_ENV=production node ./build/build.js && framework7 cordova build",
    "build-dev-cordova-ios": "cross-env TARGET=cordova cross-env NODE_ENV=development node ./build/build.js && framework7 cordova build ios",
    "build-prod-cordova-ios": "cross-env TARGET=cordova cross-env NODE_ENV=production node ./build/build.js && framework7 cordova build ios",
    "build-dev-cordova-android": "cross-env TARGET=cordova cross-env NODE_ENV=development node ./build/build.js && cd cordova && cordova build android",
    "build-prod-cordova-android": "cross-env TARGET=cordova cross-env NODE_ENV=production node ./build/build.js && cd cordova && cordova build android",
    "postinstall": "cpy ./node_modules/framework7-icons/fonts/*.* ./src/fonts/",
    "standard": "standard",
    "standard-fix": "npx standard --fix",
    "standard-html": "npx standard --plugin html '**/*.html'",
    "standard-html-fix": "npx standard --plugin html '**/*.html' --fix"
  },
  "browserslist": [
    "Android >= 7",
    "IOS >= 11",
    "Safari >= 11",
    "Chrome >= 49",
    "Firefox >= 31",
    "Samsung >= 5"
  ],
  "dependencies": {
    "@capacitor/cli": "^2.3.0",
    "@capacitor/core": "^2.3.0",
    "@capacitor/ios": "^2.3.0",
    "dom7": "^2.1.5",
    "framework7": "^5.7.8",
    "framework7-icons": "^3.0.1",
    "moment": "^2.27.0",
    "template7": "^1.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/runtime": "^7.10.2",
    "babel-loader": "^8.1.0",
    "chalk": "^4.1.0",
    "copy-webpack-plugin": "^6.0.2",
    "cpy-cli": "^3.1.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "eslint-plugin-html": "^6.0.2",
    "file-loader": "^6.0.0",
    "framework7-component-loader": "^2.1.1",
    "html-webpack-plugin": "^4.3.0",
    "ignore-loader": "^0.1.2",
    "mini-css-extract-plugin": "^0.9.0",
    "npx-run": "^2.1.2",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "ora": "^4.0.4",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "rimraf": "^3.0.2",
    "standard": "^14.3.4",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^3.0.6",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.0.9"
  },
  "standard": {
    "globals": [
      "atob",
      "Camera",
      "fetch",
      "File",
      "FileReader",
      "FormData",
      "window.sessionStorage", 
      "API_URL"
    ],
    "ignore": [
      "/src/js/cordova-app.js",
      "/src/template7-helpers-list.js",
      "/postcss.config.js",
      "/build/webpack.config.js",
      "/build/build.js",
      "/babel.config.js"
    ]
  }
}
